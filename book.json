{
        "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "-highlight",
        "-livereload",
        "simple-page-toc",
        "sectionx",
        "tbfed-pagefooter",
        "todo",
        "toggle-chapters",
        "back-to-top-button",
        "code",
        "simple-page-toc",
        "page-treeview"
        ],
        "pluginsConfig": {
        "include-codeblock": {
                "template": "ace",
                "unindent": true,
                "edit": true
        },
        "tbfed-pagefooter": {
                "copyright": "Copyright © C4xx 2020",
                "modify_label": "该文件修订时间：",
                "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
                "maxDepth": 3,
                "skipFirstH1": true
        },    
        "sectionx": {
                "tag": "b"
        }
        }
}
